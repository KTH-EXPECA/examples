        {
          "id": "renameByRegex",
          "options": {
            "regex": "^_value.*",
            "renamePattern": "CDF"
          }
        },
        {
          "id": "renameByRegex",
          "options": {
            "regex": "^le.*",
            "renamePattern": "Delay"
          }
        },
        {
          "id": "calculateField",
          "options": {
            "binary": {
              "left": "CDF",
              "operator": "*",
              "right": "-1"
            },
            "mode": "binary",
            "reduce": {
              "reducer": "sum"
            }
          }
        },
        {
          "id": "calculateField",
          "options": {
            "alias": "CCDF",
            "binary": {
              "left": "CDF * -1",
              "right": "1"
            },
            "mode": "binary",
            "reduce": {
              "reducer": "sum"
            }
          }
        },
        {
          "id": "filterFieldsByName",
          "options": {
            "include": {
              "names": [
                "Delay",
                "CCDF"
              ]
            }
          }
        }
